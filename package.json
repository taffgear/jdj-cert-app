{
  "name": "jdj-cert-process-manager",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.17.1",
    "electron-debug": "^1.5.0",
    "electron-is-dev": "^0.3.0",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "papaparse": "^4.3.7",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-csv": "^1.0.12",
    "react-dom": "^15.5.4",
    "react-dropzone": "^4.2.7",
    "react-scrollbar": "^0.5.1",
    "shortid": "^2.2.8",
    "socket.io-client": "^2.0.4",
    "string": "^3.3.3",
    "wait-on": "^2.1.0"
  },
  "version": "0.1.1",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-react-app": "^2.2.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.1",
    "electron": "^1.8.2",
    "electron-builder": "^20.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "react-dev-utils": "^1.0.0",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "export NODE_ENV=production && rm -rf ./build && webpack --env.production --optimize-minimize",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:9000 && electron .\"",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "public/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "main": "public/electron.js",
  "author": {
    "name": "M. Horsman",
    "email": "deadelus1988@gmail.com"
  }
}
